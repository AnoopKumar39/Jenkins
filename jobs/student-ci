pipeline {
    agent {
        node {
            label 'MAVEN'
        }
    }
      stages {
          stage('Cloning Repo') {
              steps {
                  git credentialsId: 'GitUserPass', url: 'https://github.com/AnoopKumar39/studentapp-ui.git'
              }
          }
          stage('Maven Compile') {
              steps {
                  sh label: '', script: 'mvn clean compile'
              }
          }
      }
}