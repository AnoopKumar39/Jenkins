pipeline {
    agent any
      stages {
          stage('Cloning Repo') {
              steps {
                  git credentialsId: 'Git-Creden', url: 'https://github.com/AnoopKumar39/studentapp-ui.git'
              }
          }
          stage('Maven Compile') {
              steps {
                  sh label: '', script: 'mvn compile'
              }
          }
      }
}